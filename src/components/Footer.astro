
---
import Image from 'astro/components/Image.astro';
import Logo from '../../public/android.webp';
const baseURL = import.meta.env.BASE_URL;

---
<footer class="bg-white dark:bg-gray-900 m-4">
    <div class="w-full max-w-screen-xl mx-auto p-4 md:py-8">
        <div class="sm:flex sm:items-center sm:justify-between">
            <a href={`${baseURL}`} class="flex items-center mb-4 sm:mb-0 space-x-3 rtl:space-x-reverse">
                <Image src={Logo} class="h-20 w-20" alt="Bola del drac" />
                <span class="self-center text-2xl font-semibold whitespace-nowrap dark:text-white">Bola del drac</span>
            </a>
            <ul class="flex flex-wrap items-center mb-6 text-sm font-medium text-gray-500 sm:mb-0 dark:text-gray-400">
                <li>

                    <a href="https://github.com/cdryampi" class="" target="_blank">
                        <span class="text-amber-500 text-3xl block">GitHub</span>
                        CDRyampi
                    </a>
                </li>
            </ul>
        </div>
    </div>
</footer>

